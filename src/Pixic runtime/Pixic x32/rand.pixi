start_timer()

RAND_MAX=32767
next=1

fn c_rand()
{
	next=next*1103515245+12345
	ret((next div 65536)%(RAND_MAX+1))
}

array=new(10000000, 1, INT32)
start=start_timer()
a=0 while a!=10000000
{
	array[a]=c_rand()
	a+1
}
printf("%u ms\n", get_timer()-start)
a=9999999 while a!=9999989
{
	printf("%i\n", array[a])
	a-1
}